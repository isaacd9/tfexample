[
  {
    "name": "${name}",
    "image": "${image}",
    "essential": true,
    "memory": 256,
    "memoryReservation": 128,
    "dockerLabels": {
      "traefik.enable": "true",
      "traefik.frontend.rule": "Host:${domain},www.${domain}"
    },
    "portMappings": [
      {
        "containerPort": "${port}"
      }
    ]
  }
]
